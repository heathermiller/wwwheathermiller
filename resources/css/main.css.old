.gradientstripe {
  width: 100%;
  background: #02daaf; /* Old browsers */
  background: -moz-linear-gradient(left,  #02daaf 0%, #5bc9cc 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, right top, color-stop(0%,#02daaf), color-stop(100%,#5bc9cc)); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(left,  #02daaf 0%,#5bc9cc 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(left,  #02daaf 0%,#5bc9cc 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(left,  #02daaf 0%,#5bc9cc 100%); /* IE10+ */
  background: linear-gradient(to right,  #02daaf 0%,#5bc9cc 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#02daaf', endColorstr='#5bc9cc',GradientType=1 ); /* IE6-9 */
  height: 8px;
}

